{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/AURA_AI/dist/index.html"
    }
  ],
  "installCommand": "npm install --prefix frontend/AURA_AI && npm install --prefix backend",
  "buildCommand": "npm run build --prefix frontend/AURA_AI",
  "outputDirectory": "dist"
}
